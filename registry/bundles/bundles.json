{
  "bundles": {
    "kokoro-82m": {
      "model_id": "kokoro-82m",
      "version": "1.0",
      "hf_repo": "xybrid-ai/Kokoro-82M-v1.0-ONNX",
      "targets": [
        {
          "platform": "universal",
          "model_file": "kokoro-v1.0.fp16.onnx",
          "model_type": "onnx",
          "source_dir": "test_models/kokoro-82m",
          "fallback": "error"
        }
      ],
      "description": "Kokoro-82M - High-quality TTS (82M params, 24 voices, 24kHz, Misaki dictionary)"
    },
    "whisper-tiny": {
      "model_id": "whisper-tiny",
      "version": "1.0",
      "hf_repo": "xybrid-ai/whisper-tiny",
      "targets": [
        {
          "platform": "universal",
          "model_file": "model.safetensors",
          "model_type": "candle",
          "source_dir": "test_models/whisper-tiny-candle",
          "fallback": "error"
        }
      ],
      "description": "Whisper-tiny ASR model (Candle runtime, SafeTensors format)"
    },
    "wav2vec2-base": {
      "model_id": "wav2vec2-base",
      "version": "1.0",
      "hf_repo": "xybrid-ai/wav2vec2-base",
      "targets": [
        {
          "platform": "universal",
          "model_file": "model.onnx",
          "model_type": "onnx",
          "source_dir": "test_models/wav2vec2-base-960h",
          "fallback": "error"
        }
      ],
      "description": "Wav2Vec2 Base 960h ASR model with CTC decoding"
    }
  }
