  "postprocessing": [
    {
      "type": "MeanPool",
      "dim": 1
    }
  ],
  "files": [
    "model.onnx",
    "vocab.txt",
    "tokenizer.json",
    "config.json"
  ],
  "metadata": {
    "task": "text-embedding",
    "embedding_dim": 384
  }
}
